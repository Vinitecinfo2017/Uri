  import java.io.IOException;

  import java.util.*;
  public class Main {
    public static void main(String[] args) {
      int valor1, valor2, soma;
      Scanner teclado = new Scanner(System.in);
      valor1 = teclado.nextInt();
      valor2 = teclado.nextInt();
      soma= valor1+valor2;
      System.out.println("X = "+soma);
    }
  }
